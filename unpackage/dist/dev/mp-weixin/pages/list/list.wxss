  margin-top: 100rpx;
}
.uni-swiper-tab {
  width: 100%;
  white-space: nowrap;
  line-height: 100rpx;
  height: 100rpx;
  position: fixed;
  background: #f7f7f7;
  z-index: 999;
  top: 0px;
  left: 0;
}
.swiper-tab-list {
  font-size: 30rpx;
  width: 150rpx;
  display: inline-block;
  text-align: center;
  color: #90b23a;
}
.swiper-tab-list .active {
    padding: 10rpx 15rpx;
    background: #7cbf00;
    color: #FFFFFF;
    -webkit-border-radius: 3px;
            border-radius: 3px;
}
