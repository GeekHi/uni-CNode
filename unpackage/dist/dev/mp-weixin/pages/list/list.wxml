<view class="uni-tab-bar"><scroll-view class="uni-swiper-tab" id="tab-bar" scroll-x scroll-left="{{scrollLeft}}"><block wx:for="{{tabBars}}" wx:for-item="tab" wx:for-index="index" wx:key="ref"><view class="swiper-tab-list" id="{{tab.ref}}" data-current="{{index}}" data-event-opts="{{[['tap',[['tapTab',[index]]]]]}}" bindtap="__e"><text class="{{[tabIndex==index?'active':'']}}">{{tab.name}}</text></view></block></scroll-view><block wx:for="{{newsList}}" wx:for-item="tabItem" wx:for-index="idx" wx:key="id"><block wx:if="{{tabIndex===idx}}"><scroll-view class="list" scroll-y data-event-opts="{{[['scrolltolower',[['loadMore',[idx]]]]]}}" bindscrolltolower="__e"><block wx:for="{{tabItem.data}}" wx:for-item="newsItem" wx:for-index="newsIndex" wx:key="newsIndex"><block><uni-media-list vue-id="{{'1-'+idx+'-'+newsIndex}}" options="{{newsItem}}" data-event-opts="{{[['^click',[['goDetail',['$0'],[[['newsList','id',tabItem.id],['data','',newsIndex]]]]]]]}}" bind:click="__e" bind:__l="__l"></uni-media-list></block></block><view class="uni-tab-bar-loading"><view class="loading-more">{{loadingText}}</view></view></scroll-view></block></block><uni-drawer vue-id="2" visible="{{drawerVisibale}}" mode="right" data-event-opts="{{[['^close',[['e0']]]]}}" bind:close="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="center"><view class="logo" hover-class="{{!login?'logo-hover':''}}" data-event-opts="{{[['tap',[['goLogin',['$event']]]]]}}" bindtap="__e"><image class="logo-img" src="{{login?uerInfo.avatarUrl:avatarUrl}}"></image><view class="logo-title"><block wx:if="{{login}}"><text class="uer-name">{{uerInfo.name}}</text></block><block wx:else><text class="uer-name">未登录</text></block></view></view><view class="center-list"><view data-event-opts="{{[['tap',[['saoyisao',['$event']]]]]}}" class="center-list-item" bindtap="__e"><uni-icon class="center-list-icon" vue-id="{{('3')+','+('2')}}" type="scan" size="18" bind:__l="__l"></uni-icon><text class="list-text" style="padding-left:6px;">扫一扫</text></view><view class="center-list-item"><text class="list-icon"></text><text class="list-text">新消息通知</text></view><view class="center-list-item"><text class="list-icon"></text><text class="list-text">关于应用</text></view><view data-event-opts="{{[['tap',[['logout',['$event']]]]]}}" class="center-list-item" bindtap="__e"><uni-icon class="center-list-icon" vue-id="{{('4')+','+('2')}}" type="minus" size="20" bind:__l="__l"></uni-icon><text class="list-text" style="padding-left:6px;">退出登录</text></view></view></view></uni-drawer><block wx:if="{{showNamePop}}"><denglu bind:close="__e" vue-id="5" data-event-opts="{{[['^close',[['handlePopClose']]]]}}" bind:__l="__l"></denglu></block></view>